<openerp>
    <data>
        
    <!-- contrat -->   
    <!-- search -->
    <record model="ir.ui.view" id="sfa_contrat_search">
            <field name="name">sfp.contrat.search</field>
            <field name="model">sfp.contrat</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Recherche des contrats">
                	<group>
	                	<filter  icon="terp-stage" string="Préinscription" domain="[('state','=','preinscription')]" help=""/>
	                	<filter  icon="terp-stage" string="En Formation" domain="[('state','=','en_formation')]" help=""/>
	                	<filter  icon="terp-stage" string="Lauréat" domain="[('state','=','laureat')]" help=""/>
	                	<filter  icon="terp-stage" string="Abandonné" domain="[('state','=','cancel')]" help=""/>
                	</group>
                	<newline/>
                    <field name="name"/>
                    <field name="date_start"/>
                    <field name="date_end"/>
                    <field name="user"/>
                    <field name="responsable"/>
                    <newline/>
                    <group expand="0" string="Grouper par ...">
                  		<filter string="De qui" icon="stock_person" domain="[]" context="{'group_by':'user'}"/>
                  		<filter string="Responsable" icon="terp-go-year" domain="[]" context="{'group_by':'responsable'}"/>
                  		<filter string="Date" icon="stock_person" domain="[]" context="{'group_by':'date_start'}"/>
                  		<filter string="CFA" icon="stock_person" domain="[]" context="{'group_by':'cfa'}"/>
                  	</group>
                 </search>
            </field>
    </record>
       
        <!--  form -->  
        <record id="view_contrat_form" model="ir.ui.view">
            <field name="name">sfp.contrat.form</field>
            <field name="model">sfp.contrat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Contrat" version="7.0">
                    <header>
                		<button name="action_training" states="draft" string="Inscrire" type="object" icon="gtk-go-forward" />
		                <button name="action_cancel" states="en_formation" string="Abandonné" type="object" icon="gtk-cancel" />
		                <button name="action_laureat" states="en_formation" string="Lauréat" type="object" icon="gtk-go-forward" />
		                <button name="action_preinscription" states="cancel" string="Remettre en brouillon" type="object" icon="terp-stock_effects-object-colorize" />
                		<field name="state" widget="statusbar" />
                	</header>
                    <sheet>
                      <group>
		                	<group>
				                <field name="name" />
				                <field name="entreprise" />
				        
				                <field name="declaration" />

				            </group>
			                <group>
			                    <field name="apprenti" />
			                    <field name="vacataire" />
			                    <field name="maitre" />

			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
		                	    
				                <field name="groupe" />
				                <field name="metier" />
				                <field name="parent_link" placeholder="cas de declaration"/>
				                
				            </group>
			                <group>
			                	<field name="responsable" placeholder="si mineur"/>
			                	<field name="user" />
			                	<field name="relation" />
			                </group>
		               </group>
		               <newline/>
		               <group>
		                	<group>
				                
				                <field name="cfa" />
				                <field name="annexe" readonly="True"/>
				                <field name="date_start" />
				                
				            </group>
			                <group>

			                	<field name="date_end" />
			                	<field name="etat" />
			                	<field name="date_abandon" />
 	
			                </group>
		               </group>
		                <notebook>
		               <page string='Factures'>
		               <field name='invoice_ids'>
		                 <field name="name" />
		                 <field name="metier" />
		                 <field name="apprenti" />
		                </field>
		               </page>
		               
		               </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_contrat_tree" model="ir.ui.view">
            <field name="name">sfp.contrat.tree</field>
            <field name="model">sfp.contrat</field>
            <field name="type">tree</field>
